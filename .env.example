# Wert.io Configuration
# Copy this file to .env and fill in your actual values

# Your Wert.io Partner ID (get this from https://partner.wert.io/)
WERT_PARTNER_ID=your_partner_id_here

# Environment: 'production' or 'sandbox'
WERT_ENVIRONMENT=production

# Optional: Webhook URL for transaction status updates
WERT_WEBHOOK_URL=https://your-domain.com/webhook/wert

# Optional: Your application domain
APP_DOMAIN=https://your-domain.com